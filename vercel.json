{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "/main.go"
    }
  ],
  "env": {
    "GO_ENV": "production"
  },
  "functions": {
    "main.go": {
      "maxDuration": 30
    }
  }
}
